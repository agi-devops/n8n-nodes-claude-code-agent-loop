{
  "name": "n8n-claude-agent-pr",
  "version": "1.1.0",
  "description": "n8n node for Claude Code Agent SDK - run AI agents with git worktrees, multi-repo support, and session resume",
  "author": {
    "name": "Danny",
    "email": "dennis@straehhuber.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/Niach/n8n-claude-agent-pr#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Niach/n8n-claude-agent-pr.git"
  },
  "bugs": {
    "url": "https://github.com/Niach/n8n-claude-agent-pr/issues"
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "claude",
    "claude-code",
    "anthropic",
    "ai",
    "agent",
    "automation",
    "git",
    "worktree"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=20.15"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": ["dist/nodes/ClaudeAgent/ClaudeAgent.node.js"]
  },
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cp nodes/ClaudeAgent/*.svg dist/nodes/ClaudeAgent/ 2>/dev/null || true",
    "dev": "tsc -w",
    "lint": "eslint nodes lib --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.0",
    "simple-git": "^3.27.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "n8n-workflow": "^1.70.0",
    "typescript": "^5.6.0"
  },
  "files": [
    "dist"
  ]
}
